{
  "name": "@dot/attendance",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3002",
    "build": "next build",
    "start": "next start --port 3002",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest tests/database",
    "test:unit": "jest --testPathIgnorePatterns=tests/database",
    "test:ui-ux": "jest --testPathPattern=ui-ux-integration",
    "test:performance": "jest --testPathPattern=performance --maxWorkers=1",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:accessibility": "playwright test --project=accessibility",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:ui-ux && npm run test:e2e",
    "test:ci": "npm run test:coverage && npm run test:e2e && npm run test:performance"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.45.4",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.21",
    "crypto-js": "^4.2.0",
    "lucide-react": "^0.543.0",
    "next": "14.2.5",
    "postcss": "^8.5.6",
    "qr-scanner": "^1.4.2",
    "qrcode": "^1.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.5.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^29.5.12",
    "@types/qrcode": "^1.5.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.2.5"
  }
}
